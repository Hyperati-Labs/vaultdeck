{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "production": {
      "autoIncrement": true,
      "ios": { "resourceClass": "medium" },
      "android": { "resourceClass": "medium", "buildType": "app-bundle" }
    },
    "production-apk": {
      "android": { "buildType": "apk" }
    }
  },
  "submit": {}
}
